//模板文件
template header
min
max
fixedFee

package drools.templates
import top.oneyi.pojo.GuatanteeCost
template "fixRate"
rule "calculate rule_@{row.rowNumber}"
dialect "mvel"
no-loop true
when
 $guaranteeCost:GuatanteeCost(amount>@{min} && amount<=@{max})
then
 modify($guaranteeCost){
     setCost(@{fixedFee})
   }
  end
 end template
